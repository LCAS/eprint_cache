           month = {June},
           title = {Discovery Channel Daily Planet feature showing use of paper "Controlled comparison of machine vision algorithms for Rumex and Urtica detection in grassland" in use by Ibex Automation Ltd.},
          author = {Charles Fox},
            year = {2016},
